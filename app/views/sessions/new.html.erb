<%= form_tag session_path do %>
<fieldset>
  <legend>Login</legend>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
  </div>

  <div>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div>
    <%= submit_tag 'Log in' %>
  </div>
</fieldset>
<% end %>
